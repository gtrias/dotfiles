[user]
	name = Genar Trias Ortiz\n
	email = genar@acs.li

[branch]
	autosetuprebase = always

[core]
	editor = vim
	excludesfile = /home/genar/.gitignore_global
	compression = 0

[merge]
	tool = diffconflicts

[mergetool "diffconflicts"]
	cmd = diffconflicts vim $BASE $LOCAL $REMOTE $MERGED
	trustExitCode = true

[mergetool]
	keepBackup = false

[push]
	default = current

[diff]
	tool = vimdiff

[difftool]
    prompt = false

[init]
	templatedir = ~/dotfiles/gittemplate

[color]
	ui = true

